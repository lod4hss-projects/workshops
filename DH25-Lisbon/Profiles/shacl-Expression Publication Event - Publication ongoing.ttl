sdh-shacl:sdh-info_C1_Shape a sh:NodeShape ;
        sh:targetClass sdh-info:C1 ;
        sh:name "Expression Publication Event";
    
        sh:property [
            sh:path rdfs:label ;
            sh:name "Label";
            sh:datatype xsd:string;
            sh:order 1; 
            sh:maxCount 1;
            ];
    
        sh:property [
            sh:path rdfs:comment ;
            sh:name "Description";
            sh:datatype xsd:string;
            sh:order 2; 
            ];

        sh:property [
            sh:path sdh-short:P1 ;
            sh:name "Standard Date-Time";
            sh:datatype xsd:string;
            sh:order 3;
            ];
    
        sh:property [
            sh:path sdh-info:P3 ;
            sh:name "published the work of" ;
            sh:class crm:E21 ;
        ];
    
        sh:property [
            sh:path sdh-info:P3 ;
            sh:name "published the work of" ;
            sh:class crm:E74 ;
        ];
    
        sh:property [
            sh:path sdh-info:P1 ;
            sh:name "used specific expression" ;
            sh:class frbroo:F2 ;
        ];
    
        sh:property [
            sh:path sdh-info:P2 ;
            sh:name "created manifestation" ;
            sh:class frbroo:F3 ;
            sh:maxCount 1 ;
        ];
    .