sdh-shacl:sdh-slc_C5_Shape a sh:NodeShape ;
        sh:targetClass sdh-slc:C5 ;
        sh:name "Membership";
    
        sh:property [
            sh:path rdfs:label ;
            sh:name "Label";
            sh:datatype xsd:string;
            sh:order 1; 
            sh:maxCount 1;
            ];
    
        sh:property [
            sh:path rdfs:comment ;
            sh:name "Description";
            sh:datatype rdf:HTML;
            sh:order 2; 
            ];

        sh:property [
            sh:path sdh-short:P1 ;
            sh:name "Standard Date-Time";
            sh:datatype xsd:string;
            sh:order 3;
            ];
    
        sh:property [
            sh:path sdh-slc:P1 ;
            sh:name "was a membership of" ;
            sh:class crm:E21 ;
            sh:maxCount 1 ;
        ];
    
        sh:property [
            sh:path sdh-slc:P1 ;
            sh:name "was a membership of" ;
            sh:class crm:E74 ;
            sh:maxCount 1 ;
        ];
    
        sh:property [
            sh:path sdh-slc:P2 ;
            sh:name "was membership in" ;
            sh:class crm:E74 ;
            sh:maxCount 1 ;
        ];
    
        sh:property [
            sh:path sdh-slc:P63 ;
            sh:name "has quality during membership" ;
            sh:class sdh-slc:C2 ;
            sh:maxCount -1 ;
        ];
    
        sh:property [
            sh:path sdh-slc:P3 ;
            sh:name "has membership type" ;
            sh:class sdh-slc:C6 ;
            sh:maxCount 1 ;
        ];
    
        sh:property [
            sh:path sdh-slc:P63 ;
            sh:name "has quality during membership" ;
            sh:class sdh-slc:C12 ;
            sh:maxCount -1 ;
        ];
    .

